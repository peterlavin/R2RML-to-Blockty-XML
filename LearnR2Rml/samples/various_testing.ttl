# Mapping created using R2RML editor.
  @prefix rr: <http://www.w3.org/ns/r2rml#> .
  @prefix foaf: <http://foaf.com/foaf/0.1/> .
  @prefix ex: <http://example.org/> .

  <#TripleMap1>
  rr:logicalTable [
    rr:tableName "employees;";
  ];
     rr:subjectMap [
      rr:template "http://example.com/employee/{id}";
      rr:class foaf:Person;
    ];

     rr:predicateObjectMap [
      rr:predicateMap [
        rr:constant foaf:based_near;
        rr:termType rr:IRI;
      ];

       rr:objectMap [
        rr:parentTriplesMap <#TripleMap2>;
        rr:joinCondition [
           rr:child "child_city";
           rr:parent "parent_city";
        ];
        rr:joinCondition [
           rr:child "child_id";
           rr:parent "parent_id";
        ];
      ];rr:objectMap [
        rr:parentTriplesMap <#TripleMap2>;
        rr:joinCondition [
           rr:child "join_2";
           rr:parent "join_2";
        ];
      ];rr:objectMap [
        rr:column "with_joins";
      ];

       rr:graphMap [
      	rr:constant ex:graph_1;
      ];
    ];
   .

  <#TripleMap2>
  rr:logicalTable [
    rr:sqlQuery """select * from employees;""";
  ];
     rr:subjectMap [
      rr:template "insert value";
      rr:class foaf:Spatial_thing;
    ];

     rr:predicateObjectMap [
      rr:predicateMap [
        rr:constant ex:label;
        rr:termType rr:IRI;
      ];

       rr:objectMap [
        rr:column "city";
      ];

     ];
   .

