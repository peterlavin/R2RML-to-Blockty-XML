  <#TripleMap1>
  rr:logicalTable [
    rr:tableName "employees";
  ];
     rr:subjectMap [
      rr:template "http://data.example.org/employee/{id}";
      rr:class foaf:Person;
      rr:termType rr:IRI;
    ];

     rr:predicateObjectMap [
      rr:predicateMap [
        rr:constant foaf:name;
        rr:termType rr:IRI;
      ];

       rr:objectMap [
        rr:column "first_name";
      ];

     ];
   .

